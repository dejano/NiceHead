<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified" targetNamespace="http://www.ftn.uns.ac.rs/xws/xsd/cb"
	xmlns:cb="http://www.ftn.uns.ac.rs/xws/xsd/cb" xmlns:cmn="http://www.ftn.uns.ac.rs/xws/xsd/common"
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb" jxb:version="2.0">

	<xs:annotation>
		<xs:appinfo>
			<jxb:globalBindings collectionType="java.util.ArrayList" />
			<jxb:schemaBindings>
				<jxb:package name="rs.ac.uns.ftn.xws.domain" />
			</jxb:schemaBindings>
		</xs:appinfo>
	</xs:annotation>

	<xs:import namespace="http://www.ftn.uns.ac.rs/xws/xsd/common"
		schemaLocation="common.xsd" />
	<xs:complexType name="Bank">
		<xs:sequence>
			<xs:element name="swiftCode" type="cmn:SwiftCode" />
			<xs:element name="bankClearingAccountNumber" type="cmn:AccountNumber" />
			<xs:element name="wsUrl" type="xs:string" />
			<xs:element name="balance">
				<xs:simpleType>
					<xs:restriction base="xs:long">
						<xs:minInclusive value="0" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BanksData">
		<xs:sequence>
			<xs:element name="banks">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="bank"
							type="cb:Bank">
							<xs:unique name="unique-bank">
								<xs:selector xpath="bank" />
								<xs:field xpath="@swiftCode" />
								<xs:field xpath="bankClearingAccountNumber" />
								<xs:field xpath="wsUrl" />
							</xs:unique>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="bank" type="cb:Bank" />
	<xs:element name="banksData" type="cb:BanksData">
	</xs:element>

</xs:schema>
